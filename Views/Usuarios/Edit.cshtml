@model CineManager.Models.Entities.Usuario
@{ ViewData["Title"] = "Editar Usuario"; }
<h1>Editar Usuario</h1>
<form asp-action="Edit" method="post">
<input type="hidden" asp-for="Id" />
<div class="form-group"><label asp-for="TipoDocumento"></label><input asp-for="TipoDocumento" class="form-control" /></div><div class="form-group"><label asp-for="NumeroDocumento"></label><input asp-for="NumeroDocumento" class="form-control" /></div><div class="form-group"><label asp-for="Nombre"></label><input asp-for="Nombre" class="form-control" /></div><div class="form-group"><label asp-for="Apellido"></label><input asp-for="Apellido" class="form-control" /></div><div class="form-group"><label asp-for="Correo"></label><input asp-for="Correo" class="form-control" /></div>
<button type="submit" class="btn btn-primary">Guardar</button>
</form>
