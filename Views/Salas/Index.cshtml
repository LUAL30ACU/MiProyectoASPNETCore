@model IEnumerable<CineManager.Models.Entities.Sala>
@{ ViewData["Title"] = "Sala"; }
<h1>Sala</h1>
<p><a asp-action="Create">Crear</a></p>
<table class="table"><thead><tr><th>Id</th><th>NumeroSala</th><th>Capacidad</th><th>Clase</th><th>SucursalId</th><th></th></tr></thead><tbody>
@foreach(var it in Model) { <tr><td>@it.Id</td><td>@it.NumeroSala</td><td>@it.Capacidad</td><td>@it.Clase</td><td>@it.SucursalId</td><td><a asp-action="Edit" asp-route-id="@it.Id">Editar</a> | <a asp-action="Delete" asp-route-id="@it.Id">Eliminar</a></td></tr> }
</tbody></table>
